RewriteBase /concert/public/

# Rule for tickets/{transaction_id}
RewriteRule ^tickets/([a-zA-Z0-9]+)$ concert/public/tickets/index.php?transaction_id=$1 [L,QSA]

# Handle 404 errors
ErrorDocument 404 /404.php

